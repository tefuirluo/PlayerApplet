<!--components/song-item-v2/song-item-v2.wxml-->
<view class="item" bindtap="onSongItemTap">
	<view class="index">{{index}}</view>
	<view class="info">
		<view class="name">
			{{itemData.name}}
		</view>
		<view class="source">
			<image class="icon" src="/assests/icons/sq_icon.png" mode=""/>
			<text>{{itemData.ar[0].name}}</text>
		</view>
	</view>
</view>
